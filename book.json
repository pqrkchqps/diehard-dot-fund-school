{
    "styles": {
        "website": "styles/website.css"
    },
    "plugins": [
        "collapsible-menu",
        "video",
        "ga",
        "youtube",
        "anchorjs",
        "meta"
    ],
    "pluginsConfig": {
        "ga": {
            "token": "UA-31046122-1",
            "configuration": {
                "alwaysSendReferrer": true
            }
        },
        "meta": {
            "name": "og:description",
            "content": "Learn how to do great online facilitation and grow a collaborative culture. Training resources, guides, and stories."
        }
    }
}